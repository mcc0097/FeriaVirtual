//The first 10 lines are the default content of a prisma schema file.
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//This code was generated by a prisma introspection of an existing database. 
model companies {
  id          Int     @id(map: "pk_companies") @default(autoincrement())
  name        String? @db.VarChar
  CIF         String? @db.VarChar(9)
  description String? @db.VarChar
  phone       Int?
  email       String? @db.VarChar
  website     String? @db.VarChar
}

model roles {
  id          Int       @id(map: "pk_role_code") @default(autoincrement())
  name        String?   @db.VarChar
  description String?   @db.VarChar
  players     players[]
}

model players {
  id        Int       @id(map: "pk_user_code") @default(autoincrement())
  user_id   String?   @db.VarChar(10)
  name      String?   @db.VarChar
  surname   String?   @db.VarChar
  birthdate DateTime? @db.Timestamp(6)
  role_id   Int?
  phone     Int?
  dni       String?   @db.VarChar(9)
  roles     roles?    @relation(fields: [role_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_role_code")
}

model wings {
  id        Int     @id(map: "pk_wing_id") @default(autoincrement())
  computing String? @db.VarChar
  leisure   String? @db.VarChar
  commerce  String? @db.VarChar
}
